{
	"name": "paircode-api",
	"version": "1.0.0",
	"description": "API of the paircode project",
	"scripts": {
		"build": "tsc",
		"start": "node build/index.js",
		"dev": "npm-run-all -p dev:*",
		"dev:server": "ts-node-dev -r tsconfig-paths/register src/server.ts",
		"dev:queue": "ts-node-dev -r tsconfig-paths/register src/queue.ts",
		"migration:create": "typeorm migration:create -n",
		"migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run"
	},
	"devDependencies": {
		"@types/bcryptjs": "^2.4.2",
		"@types/bull": "^3.14.2",
		"@types/cors": "^2.8.7",
		"@types/express": "^4.17.7",
		"@types/jsonwebtoken": "^8.5.0",
		"@types/multer": "^1.4.4",
		"@types/node": "^8.0.29",
		"@types/nodemailer": "^6.4.0",
		"npm-run-all": "^4.1.5",
		"ts-node": "3.3.0",
		"ts-node-dev": "^1.0.0-pre.59",
		"tsconfig-paths": "^3.9.0",
		"typescript": "^3.9.0"
	},
	"dependencies": {
		"@google-cloud/storage": "^5.3.0",
		"bcryptjs": "^2.4.3",
		"bull": "^3.18.0",
		"class-transformer": "^0.3.1",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"helmet": "^4.1.1",
		"jsonwebtoken": "^8.5.1",
		"multer": "^1.4.2",
		"nodemailer": "^6.4.11",
		"pg": "^8.3.2",
		"reflect-metadata": "^0.1.10",
		"typeorm": "0.2.25"
	}
}
